<h1>Admin::Breeds#index</h1>
<p>Find me in app/views/admin/breeds/index.html.erb</p>
<div class="container">
  <div class="row">
    <%= render 'admin/breeds/new_breed', breed: @breed %>
    <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#add-new-breeds">Launch demo modal</button>
  </div>
  <div class="row">
    <div class="col">
      <table class="table">
        <thead>
          <tr>
            <th>猫種</th>
          </tr>
        </thead>
        <tbody>
          <% @breeds.each do |breed| %>
            <tr>
              <td><%= breed.name %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>