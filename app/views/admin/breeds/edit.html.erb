<div class="container body">
  <div class="row">
    <div class="col-lg-8 offset-lg-2">
      <%= form_with model: @breed, url: admin_breed_path do |f| %>
        <div class="form-group">
          <%= f.label :猫種, class: "form-label" %>
          <%= f.text_field :name, class: "form-control" %>
        </div>
        <div class="form-group">
          <%= f.submit "更新", class: "btn-blue" %>
        </div>
      <% end %>
    </div>
  </div>
</div>